{
  "name": "clubhouse",
  "productName": "Clubhouse",
  "version": "0.25.0",
  "description": "A place to hangout with your agent BFFs",
  "main": ".webpack/main",
  "private": true,
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "eslint .",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:unit": "vitest run --project main --project shared",
    "test:components": "vitest run --project renderer",
    "test:e2e": "npx playwright test",
    "typecheck": "tsc --noEmit",
    "validate": "npm run typecheck && npm test && npm run make && npm run test:e2e",
    "postinstall": "node scripts/fix-node-pty-win.js"
  },
  "keywords": [],
  "author": {
    "name": "Mason Allen",
    "email": "me@mason-allen.com"
  },
  "license": "MIT",
  "devDependencies": {
    "@electron-forge/cli": "^7.11.1",
    "@electron-forge/maker-deb": "^7.11.1",
    "@electron-forge/maker-dmg": "^7.11.1",
    "@electron-forge/maker-rpm": "^7.11.1",
    "@electron-forge/maker-squirrel": "^7.11.1",
    "@electron-forge/maker-zip": "^7.11.1",
    "@electron-forge/plugin-auto-unpack-natives": "^7.11.1",
    "@electron-forge/plugin-fuses": "^7.11.1",
    "@electron-forge/plugin-webpack": "^7.11.1",
    "@electron/fuses": "^1.8.0",
    "@eslint/js": "^9.39.2",
    "@playwright/test": "^1.58.2",
    "@tailwindcss/postcss": "^4.1.18",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^19.2.13",
    "@types/react-dom": "^19.2.3",
    "@vercel/webpack-asset-relocator-loader": "^1.7.3",
    "@vitest/coverage-v8": "^4.0.18",
    "css-loader": "^7.1.3",
    "electron": "40.4.1",
    "eslint": "^9.39.2",
    "eslint-plugin-import-x": "^4.16.1",
    "fork-ts-checker-webpack-plugin": "^9.1.0",
    "jsdom": "^28.1.0",
    "monaco-editor-webpack-plugin": "^7.1.1",
    "node-loader": "^2.1.0",
    "postcss": "^8.5.6",
    "postcss-loader": "^8.2.1",
    "style-loader": "^4.0.0",
    "tailwindcss": "^4.1.18",
    "ts-loader": "^9.5.4",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.55.0",
    "vitest": "^4.0.18"
  },
  "dependencies": {
    "@monaco-editor/react": "^4.7.0",
    "@xterm/addon-fit": "^0.11.0",
    "@xterm/xterm": "^6.0.0",
    "electron-squirrel-startup": "^1.0.1",
    "highlight.js": "^11.11.1",
    "marked": "^17.0.2",
    "monaco-editor": "^0.55.1",
    "node-pty": "^1.1.0",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "zustand": "^5.0.11"
  }
}
